fun main(){
    
    val obj1=Person(name="devi", age= 19)
    obj1.canVote(age=19)
}

class Person(var name:String, var age:Int){
   
    fun canVote(age:Int){
        if(age<18){
            println("$name cannot vote")
        }else{
            println("$name can vote")
        }
    
    }
    
}
